#include <stdio.h>
#include <math.h>

int main ()
{
    char str[6];
    int sum=0;
    scanf("%s", str);
    for(int i=0; i<6; i++)
    {
        if(str[i]=='a' || str[i]=='e' || str[i]=='i' || str[i]=='o' || str[i]=='u')
            sum++;
    }
    int output = pow(2, sum);
    printf("%d", output);

    return 0;
}
